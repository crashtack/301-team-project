{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "code"
  },
  "ignore": [ "**/node_modules/**"],
  "rewrites": [ {
    "source": "**",
    "destination": "/index.html"
  } ],
  "redirects": [ {
	  "source" : "/list",
	  "destination" : "/",
	  "type" : 301
  }, {
	  "source" : "/info/*",
	  "destination" : "list",
	  "type" : 302
  } ]
  /*added rewrite section to handle page refreshing*/
}
